<?xml version="1.0" encoding="UTF-8"?>
<nodes xmlns="http://www.comegg.com/network"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.comegg.com/network network.xsd">
    <acceptor name="blogServer" commandId="command" protocol="blog"
              localPort="9000" sizeId="size" maxConnections="100">
        <identity identity="blogServer.registerUserId" pdu="registerUser"
                  direction="in"/>
        <identity identity="blogServer.registerUserAckId" pdu="registerUserAck"
                  direction="out"/>
        <identity identity="blogServer.verifyUserId" pdu="verifyUser"
                  direction="in"/>
        <identity identity="blogServer.verifyUserAckId" pdu="verifyUserAck"
                  direction="out"/>
        <identity identity="blogServer.updateUserInfoId" pdu="updateUserInfo"
                  direction="in"/>
        <identity identity="blogServer.updateUserInfoAckId"
                  pdu="updateUserInfoAck" direction="out"/>
        <identity identity="blogServer.getUserInfoId" pdu="getUserInfo"
                  direction="in"/>
        <identity identity="blogServer.getUserInfoAckId" pdu="getUserInfoAck"
                  direction="out"/>
        <identity identity="blogServer.addBlogId" pdu="addBlog" direction="in"/>
        <identity identity="blogServer.addBlogAckId" pdu="addBlogAck"
                  direction="out"/>
        <identity identity="blogServer.addCommentsId" pdu="addComments"
                  direction="in"/>
        <identity identity="blogServer.addCommentsAckId" pdu="addCommentsAck"
                  direction="out"/>
        <identity identity="blogServer.getBlogId" pdu="getBlog" direction="in"/>
        <identity identity="blogServer.getBlogAckId" pdu="getBlogAck"
                  direction="out"/>
        <identity identity="blogServer.getBlogListByUserId"
                  pdu="getBlogListByUser" direction="in"/>
        <identity identity="blogServer.getBlogListByUserAckId"
                  pdu="getBlogListByUserAck" direction="out"/>
        <identity identity="blogServer.getBlogListId" pdu="getBlogList"
                  direction="in"/>
        <identity identity="blogServer.getBlogListAckId" pdu="getBlogListAck"
                  direction="out"/>
    </acceptor>
</nodes>
